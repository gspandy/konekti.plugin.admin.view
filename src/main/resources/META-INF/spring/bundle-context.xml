<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- regular spring configuration file defining the beans for this bundle. 
		The configuration of OSGi definitions is kept in a separate configuration 
		file so that this file can easily be used for integration testing outside 
		of an OSGi environment -->

	<bean id="pluginGridViewDescriptor"
		class="com.thingtrack.konekti.plugin.admin.view.internal.PluginGridViewDescriptor" />

	<bean id="userGridViewDescriptor"
		class="com.thingtrack.konekti.plugin.admin.view.internal.UserGridViewDescriptor" />

	<bean id="newUserViewDescriptor"
		class="com.thingtrack.konekti.plugin.admin.view.internal.NewUserViewDescriptor" />

	<bean id="exampleView"
		class="com.thingtrack.konekti.plugin.admin.view.internal.ExampleView" />

</beans>